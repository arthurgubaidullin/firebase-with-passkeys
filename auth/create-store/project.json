{
  "name": "auth-create-store",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "auth/create-store/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/auth/create-store",
        "main": "auth/create-store/src/index.ts",
        "tsConfig": "auth/create-store/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "auth/create-store/**/*.ts",
          "auth/create-store/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/auth/create-store"
      }
    }
  },
  "tags": ["tier:use-case"]
}

{
  "name": "user-auth-repository",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "auth/user-repository/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/auth/user-repository",
        "main": "auth/user-repository/src/index.ts",
        "tsConfig": "auth/user-repository/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "auth/user-repository/**/*.ts",
          "auth/user-repository/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/auth/user-repository"
      }
    }
  },
  "tags": []
}

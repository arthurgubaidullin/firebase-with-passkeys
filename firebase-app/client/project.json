{
  "name": "firebase-app-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "firebase-app/client/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/firebase-app/client",
        "main": "firebase-app/client/src/index.ts",
        "tsConfig": "firebase-app/client/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "firebase-app/client/**/*.ts",
          "firebase-app/client/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/firebase-app/client"
      }
    }
  },
  "tags": []
}

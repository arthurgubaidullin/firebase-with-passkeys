{
  "name": "passkeys-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "passkeys/api/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/passkeys/api",
        "main": "passkeys/api/src/index.ts",
        "tsConfig": "passkeys/api/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "passkeys/api/**/*.ts",
          "passkeys/api/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/passkeys/api"
      }
    }
  },
  "tags": ["tier:api"]
}
